# 노션 API 설정
# https://www.notion.so/my-integrations 에서 발급
NOTION_API_KEY=secret_your_api_key_here

# 노션 데이터베이스 ID
# 데이터베이스 URL에서 복사: https://www.notion.so/workspace/{database_id}?v=...
NOTION_DATABASE_ID=your_database_id_here

# 선택사항: 기존 문서 참고용 페이지 ID (콤마로 구분)
REFERENCE_PAGE_IDS=page_id_1,page_id_2
